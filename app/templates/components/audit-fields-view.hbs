<table class="object-list-view ui unstackable celled {{if readonly 'readonly'}} table">
  <thead>
    <tr>
      <th>{{nameColumnCaption}}</th>
      <th>{{oldValueColumnCaption}}</th>
      <th>{{newValueColumnCaption}}</th>
    </tr>
  </thead>
  <tbody>
    {{#each auditFields as |auditField|}}
      <tr>
        <td>
        {{auditField.field}}
        </td>
        <td>
        {{auditField.oldValue}}
        </td>
        <td>
        {{auditField.newValue}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>